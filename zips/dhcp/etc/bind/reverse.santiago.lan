;
; BIND data file for lan loopback interface
;
$TTL	604800
@	IN	SOA	lanhost. root.localhost. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	primary.santiago.lan
@	IN	A	127.0.0.1
@	IN	A	10.20.117.2	
@	IN	AAAA	::1
40 	IN	PTR	primary.santiago.lan
50	IN	PTR	client.santiago.lan
